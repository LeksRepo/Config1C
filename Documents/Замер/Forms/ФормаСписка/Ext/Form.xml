			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументЗамер.АдресЗамера,
	ДокументЗамер.Замерщик,
	ДокументЗамер.ИмяЗаказчика,
	ДокументЗамер.Телефон,
	ДокументЗамер.Номер,
	ДокументЗамер.Офис КАК Офис,
	ДокументЗамер.Дата КАК ДатаЗамера,
	ДокументЗамер.Автор,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ Спецификация.Ссылка) КАК КоличествоСпецификаций,
	ВЫБОР
		КОГДА КОЛИЧЕСТВО(РАЗЛИЧНЫЕ Файлы.Ссылка) &gt; 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК ПрикрепленныеФайлы,
	ДокументЗамер.Ссылка
ИЗ
	Документ.Замер КАК ДокументЗамер
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.Спецификация КАК Спецификация
		ПО ДокументЗамер.Ссылка = Спецификация.ДокументОснование
			И (Спецификация.Проведен)
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Файлы КАК Файлы
		ПО (Файлы.ВладелецФайла = ДокументЗамер.Ссылка)

СГРУППИРОВАТЬ ПО
	ДокументЗамер.АдресЗамера,
	ДокументЗамер.Замерщик,
	ДокументЗамер.ИмяЗаказчика,
	ДокументЗамер.Телефон,
	ДокументЗамер.Номер,
	ДокументЗамер.Офис,
	ДокументЗамер.Автор,
	ПОДСТРОКА(ДокументЗамер.Комментарий, 1, 40),
	ДокументЗамер.Дата,
	ДокументЗамер.Ссылка</QueryText>
				<MainTable>Document.Замер</MainTable>
				<ListSettings>
					<dcsset:filter>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>dfcece9d-5077-440b-b6b3-45a5cb4538eb</dcsset:userSettingID>
					</dcsset:filter>
					<dcsset:order>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>88619765-ccb3-46c6-ac52-38e9c992ebd4</dcsset:userSettingID>
					</dcsset:order>
					<dcsset:conditionalAppearance>
						<dcsset:viewMode>Normal</dcsset:viewMode>
						<dcsset:userSettingID>b75fecce-942b-4aed-abc9-e6a02e460fb3</dcsset:userSettingID>
					</dcsset:conditionalAppearance>
				</ListSettings>
			</Settings>
		</Attribute>
