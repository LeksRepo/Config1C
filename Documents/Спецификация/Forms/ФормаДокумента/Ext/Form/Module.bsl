&НаКлиенте
Процедура ПослеЗаписи(ПараметрыЗаписи)
	
	СтатусСпецификации = ПолучитьСтатусСпецификации(Объект.Ссылка);
	ПересчитатьСуммы(); // СуммаДокумента обновляется только после записи объекта
	
	ЕстьМонтаж = Объект.ПакетУслуг = ПредопределенноеЗначение("Перечисление.ПакетыУслуг.ДоставкаДоКлиентаИМонтаж");
	ДнейДоИзготовления = (Объект.ДатаИзготовления - НачалоДня(ТекущаяДата()))/86400;
	
	
	Если СтатусСпецификации = ПредопределенноеЗначение("Перечисление.СтатусыСпецификации.Сохранен") 
		И ЕстьМонтаж 
		И ЗначениеЗаполнено(Объект.ДатаМонтажа) Тогда 
		
		Если ДнейДоИзготовления < Объект.ДнейНаИзготовлениеПоНоменклатуре Тогда
			
			Стр = ПолучитьКоличествоДнейНаИзготовление(Объект.Ссылка);
			ПодходящаяДата = НачалоДня(ТекущаяДата()) + Стр.ДнейНаИзготовление * 86400; 
			
			Режим = РежимДиалогаВопрос.ОК;
			Текст = "Время изготовления по номенклатуре: " + Стр.Номенклатура + Символы.ПС + "составляет: " + Стр.ДнейНаИзготовление + " дней." + Символы.ПС
			+ "Установите дату изготовления не ранее: " + Формат(ПодходящаяДата,"ДФ=dd.MM.yyyy");
			Вопрос(Текст, Режим, 0);
			
		ИначеЕсли ДнейДоИзготовления < 7 Тогда
			
			ПодходящаяДата = НачалоДня(ТекущаяДата()) + 7 * 86400; 
			
			Режим = РежимДиалогаВопрос.ОК;
			Текст = "Установите дату изготовления не ранее: " + Формат(ПодходящаяДата,"ДФ=dd.MM.yyyy");
			Вопрос(Текст, Режим, 0);
			
		КонецЕсли;
		
	КонецЕсли;
	
КонецПроцедуры

