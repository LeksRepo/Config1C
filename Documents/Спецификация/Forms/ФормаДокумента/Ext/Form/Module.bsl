&НаСервере
Функция ПроверитьДоступностьПодразделениямНаСервере()//  Номенклатура, Подразделение
	
	СписокНоменклатуры = Объект.Комплектация.Выгрузить(, "Номенклатура");
	Запрос = Новый Запрос;
	Запрос.Текст = 
	"ВЫБРАТЬ
	|	НоменклатураПодразделений.Номенклатура,
	|	НоменклатураПодразделений.Доступность
	|ИЗ
	|	РегистрСведений.НоменклатураПодразделений КАК НоменклатураПодразделений
	|ГДЕ
	|	НоменклатураПодразделений.Подразделение = &Подразделение
	|	И НоменклатураПодразделений.Номенклатура В(&СписокНоменклатуры)";
	
	Запрос.УстановитьПараметр("Подразделение", Объект.Подразделение);
	Запрос.УстановитьПараметр("СписокНоменклатуры", СписокНоменклатуры);
	
	РезультатЗапроса = Запрос.Выполнить();
	
	Если РезультатЗапроса.Пустой() Тогда
		
		Возврат Ложь;
		
	КонецЕсли;
	
	ВыборкаДетальныеЗаписи = РезультатЗапроса.Выбрать();
	
	Пока ВыборкаДетальныеЗаписи.Следующий() Цикл
		
		СчетчикСовпадений = 0;
		
		Для каждого Строка Из Объект.Комплектация Цикл
			
			Если Строка.Номенклатура = ВыборкаДетальныеЗаписи.Номенклатура Тогда
				
				СчетчикСовпадений = СчетчикСовпадений + 1;
				
				Если НЕ ВыборкаДетальныеЗаписи.Доступность Тогда
					
					Сообщение = Новый СообщениеПользователю;
					Сообщение.Текст = "Номенклатура "+ ВыборкаДетальныеЗаписи.Номенклатура + " не доступна для текущего подразделения";
					Сообщение.Поле = "" + Строка.Номенклатура;
					Сообщение.Сообщить();
					Возврат Истина;
					
				КонецЕсли;
				
			КонецЕсли;
			
		КонецЦикла;
		
		Если СчетчикСовпадений = 0 Тогда
			
			Сообщение = Новый СообщениеПользователю;
			Сообщение.Текст = "Номенклатура "+ ВыборкаДетальныеЗаписи.Номенклатура + " не доступна для текущего подразделения";
			Сообщение.Поле = "" + Строка.Номенклатура;
			Сообщение.Сообщить();
			Возврат Истина;
			
		КонецЕсли;
		
	КонецЦикла;
	
КонецФункции // ПроверитьДоступностьПодразделениямНаСервере()


