					Сообщить("Разная кромка фасада. Ошибка: № 005.");
				КонецЕсли;
				
			КонецЕсли;
			
			ТорцеваяЛента = Новый Структура;
			ТорцеваяЛента.Вставить("ТорцеваяЛентаСверху", НоваяСтрока.ВыборМебельнойКромкиСверху);
