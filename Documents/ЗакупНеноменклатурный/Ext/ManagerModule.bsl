
Процедура Печать(ТабДок, Ссылка) Экспорт
	//{{_КОНСТРУКТОР_ПЕЧАТИ(Печать)
	Макет = Документы.ЗакупНеноменклатурный.ПолучитьМакет("Печать");
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	ЗакупНеноменклатурный.Автор,
	|	ЗакупНеноменклатурный.Дата,
	|	ЗакупНеноменклатурный.Номер,
	|	ЗакупНеноменклатурный.Подразделение,
	|	ЗакупНеноменклатурный.НеноменклатурныйМатериал.(
	|		НомерСтроки,
	|		Спецификация,
	|		Номенклатура,
	|		Наименование,
	|		ЕдиницаИзмерения,
	|		ГдеКупить,
	|		Количество
	|	)
	|ИЗ
	|	Документ.ЗакупНеноменклатурный КАК ЗакупНеноменклатурный
	|ГДЕ
	|	ЗакупНеноменклатурный.Ссылка В (&Ссылка)";
	Запрос.Параметры.Вставить("Ссылка", Ссылка);
	Выборка = Запрос.Выполнить().Выбрать();

	ОбластьЗаголовок = Макет.ПолучитьОбласть("Заголовок");
	Шапка = Макет.ПолучитьОбласть("Шапка");
	ОбластьНеноменклатурныйМатериалШапка = Макет.ПолучитьОбласть("НеноменклатурныйМатериалШапка");
	ОбластьНеноменклатурныйМатериал = Макет.ПолучитьОбласть("НеноменклатурныйМатериал");
	ТабДок.Очистить();

	ВставлятьРазделительСтраниц = Ложь;
	Пока Выборка.Следующий() Цикл
		Если ВставлятьРазделительСтраниц Тогда
			ТабДок.ВывестиГоризонтальныйРазделительСтраниц();
		КонецЕсли;

		ТабДок.Вывести(ОбластьЗаголовок);

		Шапка.Параметры.Заполнить(Выборка);
		ТабДок.Вывести(Шапка, Выборка.Уровень());

		ТабДок.Вывести(ОбластьНеноменклатурныйМатериалШапка);
		ВыборкаНеноменклатурныйМатериал = Выборка.НеноменклатурныйМатериал.Выбрать();
		Пока ВыборкаНеноменклатурныйМатериал.Следующий() Цикл
			ОбластьНеноменклатурныйМатериал.Параметры.Заполнить(ВыборкаНеноменклатурныйМатериал);
			ТабДок.Вывести(ОбластьНеноменклатурныйМатериал, ВыборкаНеноменклатурныйМатериал.Уровень());
		КонецЦикла;

		ВставлятьРазделительСтраниц = Истина;
	КонецЦикла;
	//}}
КонецПроцедуры
