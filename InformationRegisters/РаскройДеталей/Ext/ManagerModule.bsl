Функция ПолучитьСтрокуРаскрояПоВсемНоменклатурамПоРанцу(Дерево, СсылкаДокумента)
	
	ЭтоСпецификация = Ложь;
	ЭтоНаряд = Ложь;
	Если ТипЗнч(СсылкаДокумента.Ссылка) = Тип("ДокументСсылка.Спецификация") Тогда
		ЭтоСпецификация = Истина;
	ИначеЕсли ТипЗнч(СсылкаДокумента.Ссылка) = Тип("ДокументСсылка.НарядЗадание") Тогда
		ЭтоНаряд = Истина;
	КонецЕсли;
	
	Подразделение = СсылкаДокумента.Подразделение;
	ДанныеДляРаскроя = "%ЛОГОТИП%☻";
	
	Если Дерево.Строки.Количество() > 0 Тогда
		
		ДанныеДляРаскроя = ФормированиеСтрокиРаскрояПоРанцу(ДанныеДляРаскроя, Дерево, СсылкаДокумента, Подразделение, ЭтоСпецификация, ЭтоНаряд);
			
	КонецЕсли;
	
	ПустойРаскрой = (ДанныеДляРаскроя = "%ЛОГОТИП%☻");
	
