		<field xsi:type="DataSetFieldField">
			<dataPath>Себестоимость</dataPath>
			<field>Себестоимость</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Себестоимость</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СуммаИзлишков</dataPath>
			<field>СуммаИзлишков</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Сумма излишков</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Докупить</dataPath>
			<field>Докупить</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Докупить</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>ДокупитьКоличество</dataPath>
			<field>ДокупитьКоличество</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Докупить количество</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>МинимальныйОстаток</dataPath>
			<field>МинимальныйОстаток</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Минимальный остаток</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>Резерв</dataPath>
			<field>Резерв</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Резерв</v8:content>
				</v8:item>
			</title>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>УстойчивыйОборот</dataPath>
			<field>УстойчивыйОборот</field>
			<title xsi:type="v8:LocalStringType">
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Устойчивый оборот</v8:content>
				</v8:item>
			</title>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ВЫБОР
		КОГДА Обороты.Субконто2.Базовый
			ТОГДА Обороты.Субконто2
		ИНАЧЕ Обороты.Субконто2.БазоваяНоменклатура
	КОНЕЦ КАК Номенклатура,
	СУММА(ВЫБОР
			КОГДА Обороты.Субконто2.Базовый
				ТОГДА ВЫРАЗИТЬ(Обороты.КоличествоОборотКт КАК ЧИСЛО(15, 2))
			ИНАЧЕ ВЫРАЗИТЬ(Обороты.КоличествоОборотКт * Обороты.Субконто2.КоэффициентБазовых КАК ЧИСЛО(15, 2))
		КОНЕЦ) КАК Количество,
	СУММА(ВЫРАЗИТЬ(Обороты.СуммаОборотКт КАК ЧИСЛО(15, 2))) КАК СуммаОборот,
	спрПодразделения.КоэффициентСкладскиеОстатки КАК Коэффициент,
	НАЧАЛОПЕРИОДА(Обороты.Период, МЕСЯЦ) КАК Период
ПОМЕСТИТЬ втОбороты2
ИЗ
	РегистрБухгалтерии.Управленческий.Обороты(ДОБАВИТЬКДАТЕ(&amp;Период, МЕСЯЦ, -4), &amp;Период, Месяц, Счет = ЗНАЧЕНИЕ(ПланСчетов.Управленческий.МатериалыНаСкладе), , Подразделение = &amp;Подразделение, КорСчет = ЗНАЧЕНИЕ(ПланСчетов.Управленческий.ОсновноеПроизводство), ) КАК Обороты
		ЛЕВОЕ СОЕДИНЕНИЕ Справочник.Подразделения КАК спрПодразделения
		ПО (спрПодразделения.Ссылка = &amp;Подразделение)

СГРУППИРОВАТЬ ПО
	ВЫБОР
		КОГДА Обороты.Субконто2.Базовый
			ТОГДА Обороты.Субконто2
		ИНАЧЕ Обороты.Субконто2.БазоваяНоменклатура
	КОНЕЦ,
	спрПодразделения.КоэффициентСкладскиеОстатки,
	НАЧАЛОПЕРИОДА(Обороты.Период, МЕСЯЦ)

ИНДЕКСИРОВАТЬ ПО
	Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Таб.Номенклатура,
	СУММА(Таб.Количество) / 4 КАК Количество,
	СУММА(Таб.СуммаОборот) / 4 КАК СуммаОборот,
	ВЫБОР
		КОГДА КОЛИЧЕСТВО(Таб.Период) = 4
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК УстойчивыйОборот,
	Таб.Коэффициент
ПОМЕСТИТЬ втОбороты
ИЗ
	втОбороты2 КАК Таб

СГРУППИРОВАТЬ ПО
	Таб.Номенклатура,
	Таб.Коэффициент

ИНДЕКСИРОВАТЬ ПО
	Таб.Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	Остатки.СуммаОстаток,
	ВЫБОР
		КОГДА Остатки.Субконто2.Базовый
			ТОГДА Остатки.Субконто2
		ИНАЧЕ Остатки.Субконто2.БазоваяНоменклатура
	КОНЕЦ КАК Номенклатура,
	ВЫБОР
		КОГДА Остатки.Субконто2.Базовый
			ТОГДА Остатки.КоличествоОстаток
		ИНАЧЕ Остатки.КоличествоОстаток * Остатки.Субконто2.КоэффициентБазовых
	КОНЕЦ КАК Количество,
	ВЫБОР
		КОГДА Остатки.КоличествоОстаток &gt; 0
			ТОГДА ВЫБОР
					КОГДА Остатки.Субконто2.Базовый
						ТОГДА Остатки.СуммаОстаток / Остатки.КоличествоОстаток
					ИНАЧЕ Остатки.СуммаОстаток / (Остатки.КоличествоОстаток * Остатки.Субконто2.КоэффициентБазовых)
				КОНЕЦ
		ИНАЧЕ 0
	КОНЕЦ КАК Себестоимость
ПОМЕСТИТЬ втОстатки
ИЗ
	РегистрБухгалтерии.Управленческий.Остатки(
			,
			Счет = ЗНАЧЕНИЕ(ПланСчетов.Управленческий.МатериалыНаСкладе),
			,
			Субконто1.ОсновнойСклад = ИСТИНА
				И Подразделение = &amp;Подразделение) КАК Остатки

ИНДЕКСИРОВАТЬ ПО
	Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВЫБОР
		КОГДА ДопЛимит.Субконто1.Базовый
			ТОГДА ДопЛимит.Субконто1
		ИНАЧЕ ДопЛимит.Субконто1.БазоваяНоменклатура
	КОНЕЦ КАК Номенклатура,
	ВЫБОР
		КОГДА ДопЛимит.Субконто1.Базовый
			ТОГДА ВЫРАЗИТЬ(ДопЛимит.КоличествоОстатокДт КАК ЧИСЛО(15, 2))
		ИНАЧЕ ВЫРАЗИТЬ(ДопЛимит.КоличествоОстатокДт * ДопЛимит.Субконто1.КоэффициентБазовых КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК Количество
ПОМЕСТИТЬ втДопЛимит
ИЗ
	РегистрБухгалтерии.Управленческий.Остатки(&amp;Период, Счет = ЗНАЧЕНИЕ(ПланСчетов.Управленческий.ДополнительныйЛимитЦеха), , Подразделение = &amp;Подразделение) КАК ДопЛимит

ИНДЕКСИРОВАТЬ ПО
	Номенклатура
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	спрНоменклатура.Ссылка КАК Номенклатура,
	ВЫРАЗИТЬ(втОбороты.Количество * втОбороты.Коэффициент КАК ЧИСЛО(15, 2)) КАК МинимальныйОстаток,
	ВЫБОР
		КОГДА номПодраздел.ПодЗаказ
			ТОГДА ЕСТЬNULL(втДопЛимит.Количество, 0)
		ИНАЧЕ ВЫРАЗИТЬ(втОбороты.Количество * втОбороты.Коэффициент + втОбороты.Количество КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК МаксимальныйОстаток,
	ВЫРАЗИТЬ(втОбороты.СуммаОборот КАК ЧИСЛО(15, 2)) КАК СуммаОборот,
	ВЫБОР
		КОГДА втОстатки.Себестоимость &gt; 0
			ТОГДА ВЫРАЗИТЬ(втОстатки.Себестоимость КАК ЧИСЛО(15, 2))
		ИНАЧЕ ВЫРАЗИТЬ(Цены.ПлановаяЗакупочная КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК Себестоимость,
	ВЫБОР
		КОГДА втОстатки.Количество ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ ВЫРАЗИТЬ(втОстатки.Количество КАК ЧИСЛО(15, 2))
	КОНЕЦ КАК ТекущийОстаток,
	номПодраздел.ПодЗаказ КАК ПодЗаказ,
	ОстаткиРезерв.Количество * ОстаткиРезерв.Номенклатура.КоэффициентБазовых КАК Резерв,
	втОбороты.УстойчивыйОборот КАК УстойчивыйОборот
ПОМЕСТИТЬ втДанные
ИЗ
	Справочник.Номенклатура КАК спрНоменклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.НоменклатураПодразделений КАК номПодраздел
		ПО спрНоменклатура.Ссылка = номПодраздел.Номенклатура
			И (номПодраздел.Подразделение = &amp;Подразделение)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.ЦеныНоменклатурыПоПодразделениям.СрезПоследних(, Подразделение = &amp;Подразделение) КАК Цены
		ПО спрНоменклатура.Ссылка = Цены.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ втОбороты КАК втОбороты
		ПО спрНоменклатура.Ссылка = втОбороты.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ втОстатки КАК втОстатки
		ПО спрНоменклатура.Ссылка = втОстатки.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ втДопЛимит КАК втДопЛимит
		ПО спрНоменклатура.Ссылка = втДопЛимит.Номенклатура
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.РезервныйЗапасМатериалов.СрезПоследних(
				,
				Подразделение = &amp;Подразделение
					И Склад.ОсновнойСклад) КАК ОстаткиРезерв
		ПО (спрНоменклатура.Ссылка = ВЫБОР
				КОГДА ОстаткиРезерв.Номенклатура.Базовый
					ТОГДА ОстаткиРезерв.Номенклатура.Ссылка
				ИНАЧЕ ОстаткиРезерв.Номенклатура.БазоваяНоменклатура
			КОНЕЦ)
ГДЕ
	(втОбороты.СуммаОборот &gt; 0
			ИЛИ втОстатки.Количество &gt; 0)
	И спрНоменклатура.Базовый
	И НЕ спрНоменклатура.НоменклатурнаяГруппа.ИгнорироватьВОтчетах
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	втДанные.Номенклатура КАК Номенклатура,
	ВЫБОР
		КОГДА втДанные.ПодЗаказ
			ТОГДА 0
		ИНАЧЕ втДанные.МинимальныйОстаток
	КОНЕЦ КАК МинимальныйОстаток,
	втДанные.МаксимальныйОстаток КАК МаксимальныйОстаток,
	втДанные.СуммаОборот КАК СуммаОборот,
	втДанные.Себестоимость КАК Себестоимость,
	втДанные.ТекущийОстаток КАК ТекущийОстаток,
	втДанные.ПодЗаказ КАК ПодЗаказ,
	ВЫБОР
		КОГДА втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток &gt; 0
			ТОГДА ВЫРАЗИТЬ(втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК Излишки,
	ВЫБОР
		КОГДА втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток &gt; 0
			ТОГДА ВЫРАЗИТЬ((втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток) * втДанные.Себестоимость КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК СуммаИзлишков,
	ВЫБОР
		КОГДА втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток &lt; 0
				И НЕ втДанные.ПодЗаказ
			ТОГДА ВЫРАЗИТЬ((втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток) * -1 КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК ДокупитьКоличество,
	ВЫБОР
		КОГДА втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток &lt; 0
				И НЕ втДанные.ПодЗаказ
			ТОГДА ВЫРАЗИТЬ((втДанные.ТекущийОстаток - втДанные.МаксимальныйОстаток) * втДанные.Себестоимость * -1 КАК ЧИСЛО(15, 2))
		ИНАЧЕ 0
	КОНЕЦ КАК Докупить,
	втДанные.Резерв КАК Резерв,
	втДанные.УстойчивыйОборот КАК УстойчивыйОборот
ИЗ
	втДанные КАК втДанные</query>
	</dataSet>
	<totalField>
		<dataPath>МаксимальныйОстаток</dataPath>
		<expression>Сумма(МаксимальныйОстаток)</expression>
	</totalField>
	<totalField>
		<dataPath>ТекущийОстаток</dataPath>
		<expression>Сумма(ТекущийОстаток)</expression>
	</totalField>
