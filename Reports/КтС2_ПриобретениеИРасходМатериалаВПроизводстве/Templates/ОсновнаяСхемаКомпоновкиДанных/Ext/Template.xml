			<dataPath>Субконто2</dataPath>
			<field>Субконто2</field>
		</field>
		<field xsi:type="DataSetFieldField">
			<dataPath>СчетКтЧтение</dataPath>
			<field>СчетКтЧтение</field>
		</field>
		<field xsi:type="DataSetFieldField">
